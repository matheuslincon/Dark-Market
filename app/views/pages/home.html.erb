<div id="home">
  <div id="home-welcome">
    <%= image_tag("logo.png", class:"logo-home")%>
    <%= link_to new_user_session_path do%>
      <h2 id="home-enter">ENTER</h2>
    <% end %>
     <%= link_to new_user_registration_path do%>
      <h5 id="home-register">DON'T HAVE AN ACCOUNT? REGISTER HERE</h5>
